{
  "name": "protondb-mobile",
  "author": "thiagopls1",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thiagopls1/protondb-mobile/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thiagopls1/protondb-mobile.git"
  },
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "expo start",
    "dev:android": "expo start --android",
    "dev:android:localhost": "expo start --android --localhost",
    "dev:ios": "expo start --ios",
    "dev:web": "expo start --web",
    "test": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest --runInBand",
    "test:watch": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest --watchAll --runInBand --verbose",
    "commit": "cz",
    "lint:prettier:check": "prettier . --check",
    "lint:prettier:fix": "prettier . --write",
    "hooks:prepare": "husky"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-navigation/bottom-tabs": "^7.3.13",
    "@react-navigation/drawer": "^7.3.12",
    "@react-navigation/native": "^7.1.9",
    "@react-navigation/native-stack": "^7.3.13",
    "expo": "~53.0.9",
    "expo-status-bar": "~2.2.3",
    "firebase": "^11.8.1",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.5.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.5.3"
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
