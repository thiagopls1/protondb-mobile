{
  "name": "protondb-mobile",
  "author": "thiagopls1",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thiagopls1/protondb-mobile/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thiagopls1/protondb-mobile.git"
  },
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "android:localhost": "expo start --android --localhost",
    "test": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest --runInBand",
    "test:watch": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest --watchAll --runInBand --verbose",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "commit": "cz",
    "lint:prettier:check": "prettier . --check",
    "lint:prettier:fix": "prettier . --write",
    "hooks:prepare": "husky"
  },
  "dependencies": {
    "expo": "~53.0.9",
    "expo-status-bar": "~2.2.3",
    "firebase": "^11.8.1",
    "react": "19.0.0",
    "react-native": "0.79.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.5.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.5.3"
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
